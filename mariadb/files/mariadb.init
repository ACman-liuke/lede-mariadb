# Copyright (C) 2010-2011 OpenWrt.org

START=95
STOP=10

BIN=/usr/sbin/mysqld_sh
USE_PROCD=1

start_service(){
	procd_open_instance
	procd_set_param command $BIN start
	procd_set_param respawn ${respawn_threshold:-3600} ${respawn_timeout:-50} ${respawn_retry:-6}
	procd_close_instance
}

stop_service(){
	$BIN stop
}

